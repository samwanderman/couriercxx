#!/bin/sh

g++ -o GPIOPortExample GPIOPortExample.cpp -lCourierCXX -lpthread
g++ -o I2CPortExample I2CPortExample.cpp -lCourierCXX -lpthread
g++ -o PWMExample PWMExample.cpp -lCourierCXX -lpthread
g++ -o SerialPortExample SerialPortExample.cpp -lCourierCXX -lpthread
g++ -o ServerExample ServerExample.cpp -lCourierCXX -lpthread -levent
g++ -o NetExample NetExample.cpp -lCourierCXX -lpthread
g++ -o BluetoothPortExample BluetoothPortExample.cpp -lCourierCXX -lpthread -lbluetooth -fpermissive
g++ -o SnifferExample SnifferExample.cpp -DDEBUG -lCourierCXX -lpthread -lpcap
